
___x_cmd_pkg_setup(){
    # If not installed, setup
    ___x_cmd_pkg_setup___min "$@"
}

___x_cmd_pkg_setup___min(){
    :
    if script="$(___x_cmd_pkg___get_script "$@" setup)"; then
        # eval setup script
        eval "$script"
    else
        :
    fi
}

___x_cmd_pkg_install___get_setup_script(){
    : get the install script

}
